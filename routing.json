[
	{
		"path": "/",
		"controller": "controllers\\home@index",
		"method":"get"
	},
	{
		"path": "contact-us",
		"controller": "controllers\\home@contact",
		"method":["post","get"]
	},
	{
		"path": "about-us",
		"controller": "controllers\\home@about",
		"method":"get"
	},
	{
		"path": "explore",
		"controller": "controllers\\explore@best",
		"method":"get"
	},
	{
		"path": "explore/lastest",
		"controller": "controllers\\explore@lastest",
		"method":"get"
	},
	{
		"path": ["copyright"],
		"controller": "controllers\\home@copyright",
		"method":"get"
	},
	{
		"path": ["explore","genre",{"type":"dynamic","name":"genre"}],
		"controller": "controllers\\explore@genre",
		"method":"get"
	},
	{
		"path": ["userpanel", "persons"],
		"controller": "controllers\\panel\\persons@listview",
		"method":"get"
	},
	{
		"path": ["userpanel", "persons", "edit", {"type":"dynamic", "name":"id", "regex":"/^(\\d+)$/"}],
		"controller": "controllers\\panel\\persons@edit"
	},
	{
		"path": ["userpanel", "persons", "add"],
		"controller": "controllers\\panel\\persons@add"
	},
	{
		"path": ["userpanel", "persons", "delete", {"type":"dynamic", "name":"id", "regex":"/^(\\d+)$/"}],
		"controller": "controllers\\panel\\persons@delete"
	},
	{
		"path": [{"type":"dynamic","name":"artist"},"albums"],
		"controller": "controllers\\artists@albums",
		"method":"get"
	},
	{
		"path": [{"type":"dynamic","name":"artist"},"albums",{"type":"dynamic","name":"album"}],
		"controller": "controllers\\artists@album",
		"method":"get"
	},
	{
		"path": [{"type":"dynamic","name":"artist"},{"type":"dynamic","name":"song"}],
		"controller": "controllers\\lyrics@view",
		"method":"get"
	},
	{
		"path": [{"type":"dynamic","name":"artist"}],
		"controller": "controllers\\artists@view",
		"method":"get"
	},
	{
		"paths": [
			"/"
		],
		"exceptions":[
			"\\packages\\base\\NotFound"
		],
		"handler": "controllers\\home@notfound"
	}
]
